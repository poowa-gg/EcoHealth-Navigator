# EcoHealth Navigator Environment Variables

# Server Configuration
PORT=3001
NODE_ENV=development

# API Keys (for production deployment)
OPENWEATHER_API_KEY=your_openweather_api_key_here
AIRNOW_API_KEY=your_airnow_api_key_here
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Database Configuration (for production)
DATABASE_URL=postgresql://username:password@localhost:5432/ecohealth
REDIS_URL=redis://localhost:6379

# AI/ML Configuration
TENSORFLOW_MODEL_PATH=./models/health_prediction_model
OPENAI_API_KEY=your_openai_api_key_here

# IoT Configuration
IOT_DEVICE_SECRET=your_iot_device_secret_here
LORAWAN_NETWORK_KEY=your_lorawan_network_key_here

# Community Features
COMMUNITY_WEBHOOK_URL=your_community_webhook_url_here
NOTIFICATION_SERVICE_KEY=your_notification_service_key_here